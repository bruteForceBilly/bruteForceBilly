{
  "version": 2,
  "builds": [
    {
      "src": "/jr/**",
      "use": "@vercel/static"
    },
    {
      "src": "/md/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/jr/(.*)",
      "dest": "/jr/$1"
    },
    {
      "src": "/md/(.*)",
      "dest": "/md/$1"
    }
  ]
}